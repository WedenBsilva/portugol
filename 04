programa {
  funcao inicio() {
    inteiro n1, n2, n3, n4 diferenca

    escreva("numero1; ")
    leia(n1)
    limpa()
    escreva("numero2; ")
    leia(n2)
    
    escreva("numero3; ")
    leia(n3)
    limpa()
    escreva("numero4; ")
    leia(n4)
    limpa()
    diferenca = (n1 * n2) - (n3 * n4)
    
    escreva("a diferenca entre os produto (n1 * n2) - (n3 * n4) = ", diferenca)
    
  }
}
