programa {
  funcao inicio() {
    inteiro idade
    escreva("Digite sua idade")
    leia(idade)

    se (idade >= 16 e idade <18)
    escreva("A pessoa apta a votar e o voto e facutativo")
    senao se (idade >= 18 e idade < 65)
    escreva("A pesooa esta apta a voltar e o voto e obrigatorio")
    senao se(idade >= 65)
    escreva("A pessoa esta apta a votar e o voto e facultativo")
    senao
    escreva("A pessoa nao esta apta a votar")
    
  }
}
